# Changes require a restart to take effect.

# Ensure CollectiveOS bins are in the path
export COLLECTIVEOS_PATH=$HOME/.local/share/collectiveos
export PATH=$COLLECTIVEOS_PATH/bin:$PATH

# Set default terminal and editor
source ~/.config/uwsm/default

# Activate mise if present on the system
collectiveos-cmd-present mise && eval "$(mise activate bash)"
